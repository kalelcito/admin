    {% extends '@Admin/layout.html.twig' %}

{% block stylesheets %}
    {{ parent() }}

{% endblock %}

{% block javascripts %}
    {{ parent() }}

{% endblock %}

    {% block body %}

    <div class="row">
        <div class="col-md-12">
            <div class="white-box" style="margin-bottom: -15px;">
                <h3 class="box-title m-b-0">Editar Registro</h3>
                {% form_theme edit_form '@Admin/form/ci_bootstrap_3_horizontal_layout.html.twig' %}
            </div>
                <div class="form-group m-b-0">
                    <div class="col-sm-12 white-box">
                        {{ form_start(edit_form) }}
                        {{ form_widget(edit_form) }}
                                                            <a class="btn btn-secondary round btn-min-width mr-1 mb-1" href="{{ path('administradores_index') }}">Regresar</a><button type="submit" class="btn btn-success waves-effect waves-light m-t-10" style="float:left;  margin:5px;" >Guardar</button>
                        {{ form_end(edit_form) }}
                        {{ form_start(delete_form) }}
                        <button type="submit" class="btn btn-red waves-effect waves-light m-t-10">Borrar</button>
                        {{ form_end(delete_form) }}
                    </div>
                </div>
        </div>
    </div>



    {% endblock %}
